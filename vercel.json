{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "pages/api/videos/upload.js": {
      "maxDuration": 30
    },
    "pages/api/courses/upload-thumbnail.js": {
      "maxDuration": 30
    }
  },
  "api": {
    "bodyParser": {
      "sizeLimit": "50mb"
    }
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1"
    }
  }
}